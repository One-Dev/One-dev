<?php
require_once 'lib/ConectorS3.php';

  $s3 = ConectorS3::conectarS3();
   $bucket='webproyecto2015/Unicenta';
   $result = $s3->putObject(array(
    'Bucket' => $bucket,
    'Key'    => $archivo,
    'Body'   => $archivo,
    'ACL'    => 'public-read',
   ));
  $data=$result->toArray();
  $object_url=$data['ObjectURL'];

  return $object_url;


$lista->s3=ConectorS3::descargaS3();
"<?php print_r($object_url); ?>"

?>
